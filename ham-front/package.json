  },
  "dependencies": {
    "@polkadot/api": "^5.3.2",
    "@polkadot/extension-dapp": "^0.39.1",
    "@polkadot/keyring": "^7.1.1",
    "@polkadot/networks": "^7.1.1",
    "@polkadot/types": "^5.3.2",
    "@polkadot/ui-keyring": "^0.84.3",
    "@polkadot/ui-settings": "^0.84.3",
    "@polkadot/util": "^7.1.1",
    "@polkadot/util-crypto": "^7.1.1",
    "query-string": "^6.13.5",
    "react": "^16.14.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "babel-preset-env": "^1.7.0",
    "react-app-rewired": "^2.1.8"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://substrate-developer-hub.github.io/substrate-front-end-template",
  "bugs": {
    "url": "https://github.com/substrate-developer-hub/substrate-front-end-template/issues"
  },
  "keywords": [
    "substrate",
    "substrate-ui",
    "polkadot-js"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src/__tests__/mock.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__tests__/mock.js"
    }
  }
}
